<header><h1>Attendances</h1></header>



<table class="table table-striped table-responsive">
  <thead>
    <tr>
      <th>Attendance</th>
      <th>Event</th>
      <th>Seats Left</th>

    </tr>
  </thead>
  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.attendee.full_name  %></td>
        <td><%= attendance.attended_event.title %></td>
        <td><%= attendance.attended_event.seats_left %></td>

      </tr>
    <% end %>
  </tbody>
</table>